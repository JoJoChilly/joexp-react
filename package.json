{
    "name": "joexp-react",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "pm2 start ./bin/www",
        "restart": "pm2 restart ./bin/www",
        "build-dev": "./node_modules/.bin/webpack --config scripts/webpack.dev.js --progress",
        "build": "./node_modules/.bin/webpack --config scripts/webpack.prod.js --mode production --progress",
        "redev": "./node_modules/.bin/webpack --config scripts/webpack.dev.js --progress -w",
        "dev": "pm2 start ./bin/www --watch & ./node_modules/.bin/webpack --config scripts/webpack.dev.js --progress -w",
        "makeblg": "./node_modules/.bin/showdown makehtml -i blogs/new.md -o blogs/output.html --noHeaderId && node ./bin/readFile.js"
    },
    "dependencies": {
        "autoprefixer": "7.1.2",
        "babel-core": "6.25.0",
        "babel-eslint": "7.2.3",
        "babel-jest": "20.0.3",
        "babel-loader": "^7.1.4",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-preset-env": "^1.6.0",
        "babel-preset-react-app": "^3.0.2",
        "babel-register": "^6.26.0",
        "babel-runtime": "6.26.0",
        "body-parser": "~1.18.2",
        "case-sensitive-paths-webpack-plugin": "2.1.1",
        "chalk": "^1.1.3",
        "clean-webpack-plugin": "^0.1.17",
        "cookie-parser": "~1.4.3",
        "css-loader": "0.28.4",
        "debug": "~2.6.9",
        "dotenv": "4.0.0",
        "eslint-config-react-app": "^2.0.0",
        "eslint-loader": "1.9.0",
        "eslint-plugin-flowtype": "2.35.0",
        "eslint-plugin-import": "2.7.0",
        "eslint-plugin-jsx-a11y": "5.1.1",
        "eslint-plugin-react": "7.1.0",
        "express": "~4.15.5",
        "file-loader": "^1.1.11",
        "fs-extra": "3.0.1",
        "html-webpack-plugin": "^3.2.0",
        "jest": "20.0.4",
        "less": "^2.7.3",
        "less-loader": "^4.1.0",
        "mini-css-extract-plugin": "^0.4.0",
        "moment": "^2.21.0",
        "morgan": "~1.9.0",
        "mysql2": "^1.5.0",
        "npm-install-webpack-plugin": "^4.0.5",
        "object-assign": "4.1.1",
        "open-browser-webpack-plugin": "^0.0.5",
        "pm2": "^2.7.2",
        "postcss-flexbugs-fixes": "3.2.0",
        "postcss-loader": "2.0.6",
        "promise": "8.0.1",
        "public": "^0.1.2",
        "pug": "2.0.0-beta11",
        "react": "15",
        "react-dev-utils": "^4.0.1",
        "react-dom": "^15.6.1",
        "react-hot-loader": "^1.3.1",
        "react-router": "^4.2.0",
        "react-router-dom": "^4.2.2",
        "semantic-ui-css": "^2.2.12",
        "semantic-ui-react": "^0.72.0",
        "sequelize": "^4.22.6",
        "serve-favicon": "^2.4.5",
        "showdown": "^1.8.2",
        "style-loader": "0.18.2",
        "sw-precache-webpack-plugin": "^0.11.5",
        "uglifyjs-webpack-plugin": "^1.2.5",
        "url-loader": "^1.0.1",
        "webpack": "^4.8.3",
        "webpack-dev-middleware": "^3.1.3",
        "webpack-dev-server": "^3.1.4",
        "webpack-manifest-plugin": "^2.0.3",
        "webpack-merge": "^4.1.1",
        "whatwg-fetch": "2.0.3"
    },
    "devDependencies": {
        "eslint": "^4.4.1",
        "eslint-config-airbnb": "^15.1.0",
        "eslint-config-airbnb-base": "^12.0.0",
        "webpack-cli": "^2.1.3"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx}"
        ],
        "setupFiles": [
            "<rootDir>/config/polyfills.js"
        ],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.js?(x)",
            "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
        ],
        "testEnvironment": "node",
        "testURL": "http://localhost",
        "transform": {
            "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
        ],
        "moduleNameMapper": {
            "^react-native$": "react-native-web"
        },
        "moduleFileExtensions": [
            "web.js",
            "js",
            "json",
            "web.jsx",
            "jsx",
            "node"
        ]
    },
    "babel": {
        "presets": [
            "react-app"
        ]
    },
    "eslintConfig": {
        "extends": "react-app"
    }
}
